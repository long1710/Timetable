<!DOCTYPE html>
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="/assets/processCL.js" type = "text/javascript"></script>
    <link href = "assets/result.css" type = "text/css" rel = "stylesheet"/>
    <title> result </title>
<body>
    <h1>Here is some of the time table suggested for you!</h1>
    <a href = 'home_page'><button> return </button></a>
    <button class= "result" >click here to show result!</button>
    <table>
        <tr>
            <th></th>
            <th>Monday</th>
            <th>Tuesday</th>
            <th>Wednesday</th>
            <th>Thursday</th>
            <th>Fridayday</th>
            <th>Saturday</th>
            <th>Sunday</th>
        </tr>
            <% for(var i = 0; i < 24; i++){%>
                <tr>
                    <td>period: <%= i %> </td>
                    <% for(var o = 0; o < 7; o++){%>
                        <td><%= list[o][i] %> </td>
                    <%} %>
                </tr>
           <% } %>
    </table>
    <p class = "show"></p> 
    <p><%= list[1][1] %></p>
</body>
</head>